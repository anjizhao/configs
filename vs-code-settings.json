{
    "workbench.colorTheme": "IDLE",
    "editor.minimap.enabled": false,
    "git.decorations.enabled": false,
    "workbench.editor.untitled.hint": "hidden",
    "editor.dragAndDrop": false,
    "editor.fontSize": 13,
    "editor.scrollBeyondLastLine": true,
    "files.trimTrailingWhitespace": true,
    "editor.formatOnPaste": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "git.countBadge": "off",
    "git.enableStatusBarSync": false,
    "scm.diffDecorations": "none",
    "scm.diffDecorationsGutterAction": "none",
    "scm.diffDecorationsGutterPattern": {
        "modified": false
    },
    "workbench.editor.decorations.badges": false,
    "workbench.editor.decorations.colors": false,
    "explorer.decorations.badges": false,
    "explorer.decorations.colors": false,
    "testing.gutterEnabled": false,
    "editor.renderLineHighlight": "gutter",
    "editor.glyphMargin": false,
    "workbench.colorCustomizations": {
        "editor.background": "#fafafa",
        "editorLineNumber.foreground": "#919191",
        "editorLineNumber.activeForeground": "#3a3a3a",
        "editor.selectionBackground": "#BAD6FD",
        "editorBracketHighlight.foreground1": "#21439C",
        "editorBracketHighlight.foreground2": "#216d9c",
        "editorBracketHighlight.foreground3": "#219c6b",
        "editorBracketHighlight.foreground4": "#6b9c21",
        "editorBracketHighlight.foreground5": "#9c9221",
        "editorBracketHighlight.foreground6": "#9c5d21",
        "statusBar.background": "#ececec",
        "statusBar.foreground": "#4f4f4f",
        "breadcrumb.background": "#fafafa",
        "breadcrumb.foreground": "#919191",
        "breadcrumb.focusForeground": "#4f4f4f",
        "statusBarItem.remoteBackground": "#BAD6FD",
        "statusBarItem.remoteForeground": "#4f4f4f",
        "editorIndentGuide.activeBackground": "#ececec",
        "editorIndentGuide.background": "#ececec",
        "tree.indentGuidesStroke": "#919191",
    },
    "workbench.editor.showIcons": false,
    "workbench.iconTheme": null,
    "editor.tokenColorCustomizations": {
        "comments": "#919191",
        "strings": "#00A33F",
        "functions": "#21439C",
        "keywords": "#FF5600"
    },
    "workbench.startupEditor": "none",
    "[python]": {
        "editor.formatOnSave": false
    },
    "python.linting.flake8Enabled": true,
    "editor.lightbulb.enabled": false,
    "search.mode": "reuseEditor",
    "search.searchEditor.reusePriorSearchConfiguration": true,
    "search.searchOnType": false,
    "editor.suggest.showIcons": false,
    "outline.icons": false,
    "breadcrumbs.showArrays": false,
    "breadcrumbs.showBooleans": false,
    "breadcrumbs.showConstants": false,
    "breadcrumbs.showEnumMembers": false,
    "breadcrumbs.showEnums": false,
    "breadcrumbs.showVariables": false,
    "breadcrumbs.showTypeParameters": false,
    "breadcrumbs.showStructs": false,
    "breadcrumbs.showProperties": false,
    "breadcrumbs.showPackages": false,
    "breadcrumbs.showOperators": false,
    "breadcrumbs.showObjects": false,
    "breadcrumbs.showNumbers": false,
    "breadcrumbs.showNull": false,
    "breadcrumbs.showNamespaces": false,
    "breadcrumbs.showKeys": false,
    "breadcrumbs.showInterfaces": false,
    "breadcrumbs.showModules": false,
    "breadcrumbs.showStrings": false,
    "breadcrumbs.showFields": false,
    "breadcrumbs.showEvents": false,
    "breadcrumbs.showConstructors": false,
    "breadcrumbs.icons": false,
    "python.formatting.provider": "none",
    "workbench.activityBar.visible": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.enabled": false,
    "gitlens.currentLine.pullRequests.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.menus": {
        "editor": {
            "blame": false,
            "clipboard": false,
            "compare": false,
            "history": false,
            "remote": false
        },
        "editorGroup": {
            "blame": false,
            "compare": false
        }
    },
    "window.zoomLevel": 1
}